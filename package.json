{
  "name": "claude-productivity-suite",
  "version": "3.0.0",
  "description": "AI-powered development framework that makes Claude Code workflows super simple for non-coders through intelligent agent specialists",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "server": "node src/server.js",
    "dev": "nodemon src/index.js",
    "dev:server": "nodemon src/server.js",
    "test": "npx playwright test",
    "test:unit": "npx jest --passWithNoTests",
    "build": "npm run test && npm run lint",
    "lint": "eslint src/",
    "format": "prettier --write src/",
    "setup": "./setup.sh"
  },
  "keywords": [
    "ai",
    "productivity",
    "claude",
    "non-coder",
    "development",
    "automation",
    "agents",
    "workflow"
  ],
  "author": "Claude Productivity Suite Team",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "express": "^4.18.2",
    "fs-extra": "^11.1.1",
    "inquirer": "^8.2.6",
    "lodash": "^4.17.21",
    "marked": "^9.1.6",
    "uuid": "^9.0.1",
    "ws": "^8.14.2",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.1",
    "@types/node": "^20.10.6",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tom2tomtomtom/claude-productivity-suite.git"
  },
  "homepage": "https://github.com/tom2tomtomtom/claude-productivity-suite",
  "bugs": {
    "url": "https://github.com/tom2tomtomtom/claude-productivity-suite/issues"
  }
}
